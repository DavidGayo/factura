{% block form_row %}
{% spaceless %}
<div class="form-group">
						<div class="row">
						<label class="col-lg-3 control-label">{{ form_label(form, label|default(null)) }}</label>
						<div class="col-lg-5 mb10">
							{{ form_errors(form) }}
							{{ form_widget(form, {'attr': {'class': 'form-control'}}) }}
						</div>
					</div>
				</div>
    
{% endspaceless %}
{% endblock form_row %}