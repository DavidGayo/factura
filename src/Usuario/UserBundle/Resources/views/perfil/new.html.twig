{% extends '::base.html.twig' %} 

{% block body -%}
<div class="panel">
	<div class="panel-heading">
		<span class="panel-title">Crear perfil</span>
	</div>
	<div class="panel-body">
    
    {{ form_start(form) }}
    <div class="form-group">
						<div class="row">
						<label class="col-lg-3 control-label">Avatar</label>
						<div class="col-lg-5 mb10">
							{{ form_errors(form.file.file) }}
							{{ form_widget(form.file.file, {'attr': {'class': 'form-control'}}) }}
						</div>
					</div>
	</div>
    {{ form_rest(form) }}
	</div>
</div>
{% endblock %}
