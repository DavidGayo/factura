{% extends '::base.html.twig' %}

{% block body -%}
 <div class="panel">
    <div class="panel-heading">
        <span class="panel-title">Ver Contacto</span>
    </div>
    <div class="panel-body">

        <div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>Nombre:</b> </p>
        </div>
        <div>
            {{ entity.nombre }}
        </div>
    </div>
</div>
<div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>Apellido Paterno:</b> </p>
        </div>
        <div>
            {{ entity.apePat }}
        </div>
    </div>
</div>
<div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>Apellido Materno:</b> </p>
        </div>
        <div>
            {{ entity.apeMat }}
        </div>
    </div>
</div>
<div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>Email:</b> </p>
        </div>
        <div>
            {{ entity.email }}
        </div>
    </div>
</div>
<div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>Nextel:</b> </p>
        </div>
        <div>
            {{ entity.nextel }}
        </div>
    </div>
</div>
<div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>Telefono 1:</b> </p>
        </div>
        <div>
            {{ entity.telefono1 }}
        </div>
    </div>
</div>
<div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>Telefono 2:</b> </p>
        </div>
        <div>
            {{ entity.telefono2 }}
        </div>
    </div>
</div>
<div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>Celular:</b> </p>
        </div>
        <div>
            {{ entity.celular }}
        </div>
    </div>
</div>
<div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>Cliente:</b> </p>
        </div>
        <div>
            {{ entity.cliente }}
        </div>
    </div>
</div>

</div>
        
          

 <div class="panel-footer">
        <div class="inline">
            <a href="{{ path('contacto_edit', { 'id': entity.id }) }}" class="btn btn-primary"><i class="fa fa-pencil"></i>&nbsp;Editar</a>
            <a class="btn btn-warning" href="{{ path('contacto') }}"><i class="fa fa-arrow-left">&nbsp;</i>Regresar</a><br /><br />
             {{ form(delete_form) }}
        </div>
    </div>
    
       

</div>
{% endblock %}
{% block js %}
<script src="{{ asset('bundles/app/vendor/plugins/sparkline/jquery.sparkline.min.js') }}"></script>
<script src="{{ asset('bundles/app/vendor/plugins/highcharts/highcharts.js') }}"></script>
<script src="{{ asset('bundles/app/vendor/plugins/circles/circles.js') }}"></script>
<script src="{{ asset('bundles/app/assets/js/demo/charts/highcharts.js') }}"></script>
{% endblock %}
