{% extends '::base.html.twig' %}

{% block body -%}
    <div class="panel panel-default">
        <div class="panel-heading">
            <span class="panel-title">Direccion Emisor</span>
        </div>

        <div class="panel-body">

            <div class="form-group">
                <div class="row">
                    <div class="col-xs-12 col-lg-2">
                        <p><b>Calle:</b></p>
                    </div>
                    <div>
                        {{ entity.calle }}
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="row">
                    <div class="col-xs-12 col-lg-2">
                        <p><b>Numero Ext:</b></p>
                    </div>
                    <div>
                        {{ entity.numeroExt }}
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="row">
                    <div class="col-xs-12 col-lg-2">
                        <p><b>Numero Int:</b></p>
                    </div>
                    <div>
                        {{ entity.numeroInt }}
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="row">
                    <div class="col-xs-12 col-lg-2">
                        <p><b>Colonia:</b></p>
                    </div>
                    <div>
                        {{ entity.colonia }}
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="row">
                    <div class="col-xs-12 col-lg-2">
                        <p><b>Localidad:</b></p>
                    </div>
                    <div>
                        {{ entity.localidad }}
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="row">
                    <div class="col-xs-12 col-lg-2">
                        <p><b>Codigo Postal:</b></p>
                    </div>
                    <div>
                        {{ entity.codigoPostal }}
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="row">
                    <div class="col-xs-12 col-lg-2">
                        <p><b>E-mail:</b></p>
                    </div>
                    <div>
                        {{ entity.email }}
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="row">
                    <div class="col-xs-12 col-lg-2">
                        <p><b>Telefono:</b></p>
                    </div>
                    <div>
                        {{ entity.telefono }}
                    </div>
                </div>
            </div>
        </div>   
        
        <div class="panel-footer">
            <div class="inline">
                <a href="{{ path('direccionemisor_edit', { 'id': entity.id }) }}" class="btn btn-primary"><i class="fa fa-pencil"></i>&nbsp;Editar</a>
                <a class="btn btn-warning" href="{{ path('direccionemisor') }}"><i class="fa fa-arrow-left">&nbsp;</i>Regresar</a><br /><br />
            </div>
        </div>

    </div>
{% endblock %}
{% block js %}
<script src="{{ asset('bundles/app/vendor/plugins/sparkline/jquery.sparkline.min.js') }}"></script>
<script src="{{ asset('bundles/app/vendor/plugins/highcharts/highcharts.js') }}"></script>
<script src="{{ asset('bundles/app/vendor/plugins/circles/circles.js') }}"></script>
<script src="{{ asset('bundles/app/assets/js/demo/charts/highcharts.js') }}"></script>
{% endblock js %}
