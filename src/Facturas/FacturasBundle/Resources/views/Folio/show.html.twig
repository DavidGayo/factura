{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Folio</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Serie</th>
                <td>{{ entity.serie }}</td>
            </tr>
            <tr>
                <th>Folioinicial</th>
                <td>{{ entity.folioInicial }}</td>
            </tr>
            <tr>
                <th>Foliofinal</th>
                <td>{{ entity.folioFinal }}</td>
            </tr>
            <tr>
                <th>Tipodocumento</th>
                <td>{{ entity.tipoDocumento }}</td>
            </tr>
            <tr>
                <th>Lugarexpedicion</th>
                <td>{{ entity.lugarExpedicion }}</td>
            </tr>
            <tr>
                <th>Preciounitario</th>
                <td>{{ entity.precioUnitario }}</td>
            </tr>
            <tr>
                <th>Modelo</th>
                <td>{{ entity.modelo }}</td>
            </tr>
            <tr>
                <th>Llave</th>
                <td>{{ entity.llave }}</td>
            </tr>
            <tr>
                <th>Llavepath</th>
                <td>{{ entity.llavePath }}</td>
            </tr>
            <tr>
                <th>Certificado</th>
                <td>{{ entity.certificado }}</td>
            </tr>
            <tr>
                <th>Certificadopath</th>
                <td>{{ entity.certificadoPath }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('folio') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('folio_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
