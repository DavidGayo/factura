{% extends '::base.html.twig' %}

{% block body -%}
<div class="panel">
    <div class="panel-heading">
        <span class="panel-title">Ver Cliente</span>
    </div>
    <div class="panel-body">

        <div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>Razon Social:</b> </p>
        </div>
        <div>
            {{ entity.razonSocial }}
        </div>
    </div>
</div>
<div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>RFC:</b> </p>
        </div>
        <div>
            {{ entity.rfc }}
        </div>
    </div>
</div>
<div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>Alias:</b> </p>
        </div>
        <div>
            {{ entity.alias }}
        </div>
    </div>
</div>
<div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>Email:</b> </p>
        </div>
        <div>
            {{ entity.email }}
        </div>
    </div>
</div>
<div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>Telefono:</b> </p>
        </div>
        <div>
            {{ entity.telefono }}
        </div>
    </div>
</div>
<div class="form-group">
                        <div class="row">
        <div class="col-xs-12 col-lg-2">
            <p><b>Regimen Tributario:</b> </p>
        </div>
        <div>
            {{ entity.regimenTributario }}
        </div>
    </div>
</div>

</div>
        
          

 <div class="panel-footer">
        <div class="inline">
            <a href="{{ path('cliente_edit', { 'id': entity.id }) }}" class="btn btn-primary"><i class="fa fa-pencil"></i>&nbsp;Editar</a>
            <a class="btn btn-warning" href="{{ path('cliente') }}"><i class="fa fa-arrow-left">&nbsp;</i>Regresar</a><br /><br />
             {{ form(delete_form) }}
        </div>
    </div> 

</div>
{% endblock %}
{% block js %}
<script src="{{ asset('bundles/app/vendor/plugins/sparkline/jquery.sparkline.min.js') }}"></script>
<script src="{{ asset('bundles/app/vendor/plugins/highcharts/highcharts.js') }}"></script>
<script src="{{ asset('bundles/app/vendor/plugins/circles/circles.js') }}"></script>
<script src="{{ asset('bundles/app/assets/js/demo/charts/highcharts.js') }}"></script>
{% endblock js %}
    