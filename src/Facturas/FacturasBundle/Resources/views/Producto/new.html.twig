 <div class="panel mb25 mt5">

            <div class="panel-heading">
              <span class="panel-title hidden-xs"> Agregar producto</span>
              <ul class="nav panel-tabs-border panel-tabs">
                <li class="active">
                  <a href="#tab1_1" data-toggle="tab">General</a>
                </li>
                 <li>
                  <a href="#tab1_2" data-toggle="tab">Descripción</a>
                </li>                
              </ul>
            </div>
            {{ form_start(form) }}
            <div class="panel-body p20 pb10">
              <div class="tab-content pn br-n admin-form">

                <div id="tab1_1" class="tab-pane active">
                  <div class="section row mbn">
                    <div class="col-md-9 pl15">

                      <div class="section row mb15"> 

                        <div class="col-xs-6">                       
                          <label for="codigoBarras" class="field prepend-icon">
                             {{ form_widget(form.codigoBarras, {'attr': {'class': 'event-name gui-input br-light light','placeholder':'Codigo de Barras'}}) }}	        
                            <label for="codigoBarras" class="field-icon">
                              <i class="fa fa-barcode"></i>
                            </label>
                          </label>
                           <span class="errorMsg">{{ form_errors(form.codigoBarras) }}</span>
                        </div>                         
                        <div class="col-xs-6">                       
                          <label for="clave" class="field prepend-icon">
                             {{ form_widget(form.clave, {'attr': {'class': 'event-name gui-input br-light light','placeholder':'Clave'}}) }}         
                            <label for="clave" class="field-icon">
                              <i class="fa fa-pencil"></i>
                            </label>
                          </label>
                           <span class="errorMsg">{{ form_errors(form.clave) }}</span>
                        </div>

                      </div>

                      <div class="section row mb15">
                        <div class="col-xs-6">
                          <label for="marca" class="field prepend-icon">
                            {{ form_widget(form.marca, {'attr': {'class': 'event-name gui-input br-light light','placeholder':'Marca'}}) }}
                            <label for="marca" class="field-icon">
                              <i class="fa fa-tag"></i>
                            </label>
                          </label>
                            <span class="errorMsg">{{ form_errors(form.marca) }}</span>
                        </div>
                        <div class="col-xs-6">                       
                          <label for="unidadMedida" class="field prepend-icon">
                             {{ form_widget(form.unidadMedida, {'attr': {'class': 'event-name gui-input br-light light','placeholder':'Unidad de Medida'}}) }}         
                            <label for="unidadMedida" class="field-icon">
                              <i class="fa fa-bar-chart-o"></i>
                            </label>
                          </label>
                           <span class="errorMsg">{{ form_errors(form.unidadMedida) }}</span>
                        </div>    
                      </div>

                      </div>
                    </div>                   
                  </div>

                   <div id="tab1_2" class="tab-pane">
                      <div class="section row mbn">
                        <div class="col-md-9 pl15">
                        
                           <div class="section row mb15">
                              <div class="col-xs-6">
                                <label for="precioUnitarioMxn" class="field prepend-icon">
                                  {{ form_widget(form.precioUnitarioMxn, {'attr': {'class': 'event-name gui-input br-light light','placeholder':'Precio Unitario(Mxn)'}}) }}
                                  <label for="precioUnitarioMxn" class="field-icon">
                                    <i class="fa fa-money"></i>
                                  </label>
                                </label>
                                  <span class="errorMsg">{{ form_errors(form.precioUnitarioMxn) }}</span>
                              </div>
                              <div class="col-xs-6">
                                <label for="precioUnitarioUsd" class="field prepend-icon">
                                  {{ form_widget(form.precioUnitarioUsd, {'attr': {'class': 'event-name gui-input br-light light','placeholder':'Precio Unitario(Usd)'}}) }}
                                  <label for="precioUnitarioUsd" class="field-icon">
                                    <i class="fa fa-money"></i>
                                  </label>
                                </label>
                                  <span class="errorMsg">{{ form_errors(form.precioUnitarioUsd) }}</span>
                              </div>
                          </div>

                           <div class="section row mb15"> 
                            <div class="col-xs-6">
                            <p>&nbsp;</p>                       
                              <label for="modelo" class="field prepend-icon">
                                 {{ form_widget(form.modelo, {'attr': {'class': 'event-name gui-input br-light light','placeholder':'Modelo'}}) }}         
                                <label for="modelo" class="field-icon">
                                  <i class="fa fa-tags"></i>
                                </label>
                              </label>
                               <span class="errorMsg">{{ form_errors(form.modelo) }}</span>
                            </div>                         
                            <div class="col-xs-6">
                            <p>Tipo Producto</p>                       
                              <label for="tipoProducto" class="field select">
                                 {{ form_widget(form.tipoProducto) }}         
                                <label for="tipoProducto" class="field-icon">
                                  <i class="arrow double"></i>
                                </label>
                              </label>
                               <span class="errorMsg">{{ form_errors(form.tipoProducto) }}</span>
                            </div>
                          </div>

                           <div class="section row mb15"> 
                            <div class="col-xs-12">                       
                              <label for="descripcion" class="field prepend-icon">
                                 {{ form_widget(form.descripcion, {'attr': {'class': 'event-name gui-input br-light light','placeholder':'Descripción'}}) }}         
                                <label for="descripcion" class="field-icon">
                                  <i class="fa fa-pencil-square-o"></i>
                                </label>
                              </label>
                               <span class="errorMsg">{{ form_errors(form.descripcion) }}</span>
                            </div>                         
                          </div>
                          
                         {{ form_widget(form._token) }}  
                         
                            <div class="col-xs-12">
                              <button class="btn btn-success" type="submit" id="sendform"><i class="fa fa-plus"></i>&nbsp;Crear</button>
                            </div>
                          
                          </form>

                        </div>
                      </div>
                    </div>

                  
          </div>
        </div>
      </div>
