<?php

namespace Facturas\GeneralBundle\Entity;

/**
 * Moneda
 */
class Moneda
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $tipoMoneda;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set tipoMoneda
     *
     * @param string $tipoMoneda
     *
     * @return Moneda
     */
    public function setTipoMoneda($tipoMoneda)
    {
        $this->tipoMoneda = $tipoMoneda;

        return $this;
    }

    /**
     * Get tipoMoneda
     *
     * @return string
     */
    public function getTipoMoneda()
    {
        return $this->tipoMoneda;
    }
}
